{
  "name": "toolkit-docs-generator",
  "version": "1.0.0",
  "description": "CLI tool for generating Arcade toolkit documentation JSON from multiple data sources",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "toolkit-docs-generator": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup src/cli/index.ts src/index.ts --format esm --dts --clean",
    "dev": "tsup src/cli/index.ts src/index.ts --format esm --watch",
    "start": "node dist/cli/index.js",
    "lint": "pnpm dlx ultracite check",
    "lint:fix": "pnpm dlx ultracite fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "check": "pnpm lint && pnpm typecheck && pnpm test"
  },
  "keywords": [
    "arcade",
    "documentation",
    "toolkit",
    "generator",
    "cli"
  ],
  "author": "Arcade",
  "license": "MIT",
  "engines": {
    "node": "22.x",
    "pnpm": ">=9.15.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.71.2",
    "chalk": "^5.4.0",
    "commander": "^14.0.2",
    "openai": "^6.7.0",
    "ora": "^9.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/node": "^24.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.9.3",
    "ultracite": "^7.0.11",
    "vitest": "^4.0.5"
  },
  "packageManager": "pnpm@10.11.0"
}
