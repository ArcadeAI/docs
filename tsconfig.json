{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@pages/*": [
        "./content/*"
      ],
      "@config": [
        "./config.ts"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@components/*": [
        "./app/components/*"
      ],
      "@markdown/*": [
        "./app/markdown/*"
      ],
      "@layouts/*": [
        "./app/layouts/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "content/home/_meta.tsz",
    "next-env.d.ts",
    "/components/pages/quickstart/prerequisites.mdx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "agents"
  ]
}
